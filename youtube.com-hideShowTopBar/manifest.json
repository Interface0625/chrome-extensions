{
	"name":"Inject DOM",
	"description":"Hide youtube top bar",
	"version":"1",
	"manifest_version":2,
	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/*","http://www.youtube.com/*"],
			"js": ["myscript.js"],
			"run_at": "document_idle"
		}
	],
	"web_accessible_resources": ["*.png"]
}